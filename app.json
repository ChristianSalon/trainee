{
  "expo": {
    "name": "Trainee",
    "slug": "Trainee",
    "scheme": "trainee",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "splash": {
      "image": "./src/assets/splash_02.png",
      "resizeMode": "cover",
      "backgroundColor": "#139874"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.christiansalon.trainee",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "package": "com.christiansalon.trainee",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow trainee to access your photos"
        }
      ],
      ["expo-notifications"]
    ]
  }
}
